<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>DosenServlet</servlet-name>
        <servlet-class>com.ubl.jwp.servlet.DosenServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetDosenServlet</servlet-name>
        <servlet-class>com.ubl.jwp.servlet.GetDosenServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DosenServlet</servlet-name>
        <url-pattern>/DosenServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddDosenServlet</servlet-name>
        <servlet-class>com.ubl.jwp.servlet.AddDosenServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddDosenServlet</servlet-name>
        <url-pattern>/AddDosenServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteDosenServlet</servlet-name>
        <servlet-class>com.ubl.jwp.servlet.DeleteDosenServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteDosenServlet</servlet-name>
        <url-pattern>/DeleteDosenServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetDosenServlet</servlet-name>
        <url-pattern>/GetDosenServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
